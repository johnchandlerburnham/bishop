Bool : Type
Bool = @self
  (0   P: Bool -> Type) ->
  (True : P (\0 P: Bool -> Type, t: @self P self, f: @self P self => t)) ->
  (False: P (\0 P: Bool -> Type ,t: @self P self, f: @self P self => f)) ->
  (P self)

True : Bool
True = \0 P: Bool -> Type, t: @self P self, f: @self P self => t

False : Bool
False = \0 P: Bool -> Type, t: @self P self, f: @self P self => f


